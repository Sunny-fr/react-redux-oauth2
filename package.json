{
  "name": "react-redux-oauth2",
  "version": "0.1.6",
  "description": "React Redux OAuth2",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "watch": "gulp watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.{js,es}",
      "!test/__lib__/**/*"
    ],
    "source": [
      "**/*.{js,jsx,es}",
      "!build/**/*"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbin/redux-oauth2.git"
  },
  "author": "Garbin Huang <garbinh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garbin/redux-oauth2/issues"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-watch": "^4.3.9",
    "nyc": "^8.1.0"
  },
  "homepage": "https://github.com/garbin/redux-oauth2#readme",
  "keywords": [
    "redux",
    "oauth2"
  ],
  "dependencies": {
    "axios": "^0.15.3",
    "cookie": "^0.3.1",
    "query-string": "^4.2.3",
    "react": "^15.4.2",
    "react-cookie": "^0.4.8",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  }
}
